{
  "name": "gulp-yarn",
  "version": "3.0.0",
  "description": "Automatically install node modules using Yarn.",
  "main": "src/index.ts",
  "typings": "index.d.ts",
  "scripts": {
    "commit": "npx git-cz --disable-emoji",
    "coveralls": "jest --coverage && cat coverage/lcov.info | coveralls",
    "release": "standard-version",
    "test": "tap test/test.ts --ts --coverage-report=html",
    "lint:standard": "npx standard | snazzy",
    "lint": "eslint . --ext .ts"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/warapitiya/gulp-yarn.git"
  },
  "keywords": [
    "gulpplugin",
    "yarn",
    "yarnpkg",
    "gulp",
    "install"
  ],
  "author": "Malindu Warapitiya <warapitiya93@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/warapitiya/gulp-yarn/issues"
  },
  "homepage": "https://github.com/warapitiya/gulp-yarn",
  "dependencies": {
    "plugin-error": "^2.0.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "@types/proxyquire": "^1.3.28",
    "@types/tap": "^15.0.7",
    "@types/vinyl": "^2.0.6",
    "@types/which": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "coveralls": "^3.0.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.7.1",
    "proxyquire": "^2.1.3",
    "quibble": "^0.6.14",
    "standard-version": "^9.0.0",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vinyl": "^2.1.0"
  }
}
